<Page x:Class="PageScreen.Dairy.Content"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PageScreen.Dairy"
      xmlns:do="clr-namespace:PageScreen.Main"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="800"
      Background="White"
      Title="Content">

    <StackPanel>
        <TextBox Margin="0,10,300,0" Width="200" Height="30" Background="Aqua" x:Name="txtyou"></TextBox>
         <TextBox Margin="0,10,0,0" x:Name="txtcontent" Height="154" Width="500" Background="Aqua"></TextBox>
        <Button Width="100" Background="Aqua" Height="30" Margin="0,30,0,0" Content="{x:Static do:Resource.sav}" Click="Button_Click" FontSize="16"></Button>
    </StackPanel>
        
    
</Page>
